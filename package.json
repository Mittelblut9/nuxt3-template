{
  "name": "nuxt3-template",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert .cert/localhost/localhost.pem --ssl-key .cert/localhost/localhost-key.pem",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start:dev": "docker compose up -d && pnpm run logs",
    "restart": "pnpm run stop && pnpm run start:dev",
    "logs": "docker compose logs -f",
    "stop": "docker compose down",
    "bash:app": "docker compose exec app bash",
    "prepare": "husky",
    "sass-migrate": "sass-migrator module --migrate-deps src/assets/scss/base.scss",
    "code:format": "eslint --fix .",
    "code:lint": "eslint ."
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^1.2.0",
    "@nuxt/eslint-config": "^1.2.0",
    "@nuxt/fonts": "^0.11.0",
    "@nuxt/image": "^1.8.1",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxtjs/i18n": "^9.1.1",
    "@nuxtjs/seo": "^3.0.0",
    "@types/js-yaml": "^4.0.9",
    "eslint": "^9.23.0",
    "husky": "^9.1.7",
    "nuxt": "^3.15.0",
    "nuxt-appwrite": "^1.1.1",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "sass-loader": "^16.0.4",
    "sass-migrator": "^2.2.1",
    "typescript": "^5.8.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.6"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^3.0.2",
    "@nuxt/ui": "^3.0.0",
    "@nuxt/ui-pro": "^3.0.0",
    "@pinia/nuxt": "^0.10.0",
    "@sentry/nuxt": "^9.0.0",
    "pinia": "^2.3.0",
    "sass": "^1.85.1",
    "vite-plugin-vue-tracer": "^0.1.3"
  }
}
