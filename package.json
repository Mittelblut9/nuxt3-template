{
  "name": "nuxt3-template",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start:dev": "docker compose up -d && npm run logs",
    "restart": "npm run stop && npm run start:dev",
    "logs": "docker compose logs -f",
    "stop": "docker compose down",
    "bash:app": "docker compose exec app bash",
    "code:eslint": "eslint --ext .ts,.vue ./src --c .tools/eslint/.eslintrc.cjs --cache --cache-location .tools/eslint/.cache/",
    "code:lint": "npm run code:eslint && npm run scss:lint",
    "code:format": "npm run code:eslint -- --fix && npm run scss:lint -- --fix",
    "scss:lint": "stylelint src/assets/scss --config .tools/stylelint/stylelintrc.json",
    "prepare": "husky"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/image": "^1.7.0",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxtjs/i18n": "^8.3.1",
    "@types/aos": "^3.0.7",
    "husky": "^9.0.11",
    "nuxt": "^3.12.3",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "sass-loader": "^14.2.1",
    "typescript": "^5.4.3",
    "vue": "^3.4.27",
    "vue-router": "^4.3.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@nuxt/typescript-runtime": "^3.0.2",
    "@nuxt/ui": "^2.17.0",
    "@nuxtjs/supabase": "^1.2.2",
    "@pinia/nuxt": "^0.5.1",
    "@sentry/browser": "7.107.0",
    "@sentry/node": "7.107.0",
    "@sentry/profiling-node": "7.107.0",
    "@supabase/gotrue-js": "^2.64.3",
    "@supabase/supabase-js": "^2.44.0",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "pinia": "^2.1.7",
    "sass": "^1.72.0"
  }
}
