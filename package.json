{
  "name": "nuxt3-template",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert .cert/localhost/localhost.pem --ssl-key .cert/localhost/localhost-key.pem",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start:dev": "docker compose up -d && npm run logs",
    "restart": "npm run stop && npm run start:dev",
    "logs": "docker compose logs -f",
    "stop": "docker compose down",
    "bash:app": "docker compose exec app bash",
    "prepare": "husky",
    "sass-migrate": "sass-migrator module --migrate-deps src/assets/scss/base.scss"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/image": "^1.8.1",
    "@nuxt/typescript-build": "^3.0.2",
    "@nuxtjs/i18n": "^9.1.1",
    "@nuxtjs/seo": "^2.0.2",
    "husky": "^9.1.7",
    "nuxt": "^3.14.1592",
    "nuxt-appwrite": "^1.1.1",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "sass-loader": "^16.0.4",
    "sass-migrator": "^2.2.1",
    "typescript": "^5.6.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^3.0.2",
    "@nuxt/ui": "^2.19.2",
    "@nuxt/ui-pro": "^1.5.0",
    "@pinia/nuxt": "^0.9.0",
    "@sentry/nuxt": "^8.42.0",
    "pinia": "^2.3.0",
    "sass": "^1.82.0"
  }
}
